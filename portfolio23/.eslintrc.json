{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "react-app"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "windows"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
